<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="id.monpres.app.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@animator/nav_default_pop_enter_anim"
            app:popExitAnim="@animator/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_editVehicleFragment"
            app:destination="@id/editVehicleFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@animator/nav_default_pop_enter_anim"
            app:popExitAnim="@animator/nav_default_pop_exit_anim"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false">
            <argument
                android:name="vehicle"
                android:defaultValue="@null"
                app:argType="id.monpres.app.model.Vehicle"
                app:nullable="true" />
        </action>
        <action
            android:id="@+id/action_homeFragment_to_quickServiceFragment"
            app:destination="@id/quickServiceFragment" />
        <action
            android:id="@+id/action_homeFragment_to_scheduledServiceFragment"
            app:destination="@id/scheduledServiceFragment" />
        <action
            android:id="@+id/action_homeFragment_to_vehicleListFragment"
            app:destination="@id/vehicleListFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@animator/nav_default_pop_enter_anim"
            app:popExitAnim="@animator/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_insertVehicleFragment"
            app:destination="@id/insertVehicleFragment" />
        <action
            android:id="@+id/action_homeFragment_to_serviceProcessFragment"
            app:destination="@id/serviceProcessFragment" />
        <action
            android:id="@+id/action_homeFragment_to_orderServiceDetailFragment"
            app:destination="@id/orderServiceDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_orderServiceListFragment"
            app:destination="@id/orderServiceListFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="id.monpres.app.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" >
        <argument
            android:name="user"
            android:defaultValue="@null"
            app:argType="id.monpres.app.model.MontirPresisiUser"
            app:nullable="true"/>
    </fragment>
    <fragment
        android:id="@+id/editVehicleFragment"
        android:name="id.monpres.app.ui.editvehicle.EditVehicleFragment"
        android:label="@string/edit_vehicle"
        tools:layout="@layout/fragment_edit_vehicle">
        <argument
            android:name="vehicle"
            android:defaultValue="@null"
            app:argType="id.monpres.app.model.Vehicle"
            app:nullable="true" />
    </fragment>

    <fragment
        android:id="@+id/quickServiceFragment"
        android:name="id.monpres.app.ui.quickservice.QuickServiceFragment"
        android:label="@string/quick_service"
        tools:layout="@layout/fragment_quick_service" >
        <action
            android:id="@+id/action_quickServiceFragment_to_serviceProcessFragment"
            app:destination="@id/serviceProcessFragment"
            app:popUpTo="@id/homeFragment" />
        <argument
            android:name="serviceId"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/scheduledServiceFragment"
        android:name="id.monpres.app.ui.scheduledservice.ScheduledServiceFragment"
        android:label="@string/scheduled_service"
        tools:layout="@layout/fragment_scheduled_service" >
        <argument
            android:name="serviceId"
            app:argType="string" />
    </fragment>
    <action
        android:id="@+id/action_global_profileFragment"
        app:destination="@id/profileFragment"
        app:enterAnim="@animator/nav_default_enter_anim"
        app:exitAnim="@animator/nav_default_exit_anim"
        app:popEnterAnim="@animator/nav_default_pop_enter_anim"
        app:popExitAnim="@animator/nav_default_pop_exit_anim"
        app:popUpTo="@id/profileFragment"
        app:popUpToInclusive="true" />
    <fragment
        android:id="@+id/vehicleListFragment"
        android:name="id.monpres.app.ui.vehiclelist.VehicleListFragment"
        android:label="@string/vehicle_list"
        tools:layout="@layout/fragment_vehicle_list">
        <action
            android:id="@+id/action_vehicleListFragment_to_editVehicleFragment"
            app:destination="@id/editVehicleFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@animator/nav_default_pop_enter_anim"
            app:popExitAnim="@animator/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_vehicleListFragment_to_insertVehicleFragment"
            app:destination="@id/insertVehicleFragment" />
    </fragment>
    <fragment
        android:id="@+id/insertVehicleFragment"
        android:name="id.monpres.app.ui.insertvehicle.InsertVehicleFragment"
        android:label="@string/add_vehicle"
        tools:layout="@layout/fragment_insert_vehicle" />
    <fragment
        android:id="@+id/orderServiceListFragment"
        android:name="id.monpres.app.ui.orderservicelist.OrderServiceListFragment"
        android:label="History"
        tools:layout="@layout/fragment_order_service_list">
        <action
            android:id="@+id/action_orderServiceListFragment_to_orderServiceDetailFragment"
            app:destination="@id/orderServiceDetailFragment" />
        <action
            android:id="@+id/action_orderServiceListFragment_to_serviceProcessFragment"
            app:destination="@id/serviceProcessFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_orderServiceListFragment"
        app:destination="@id/orderServiceListFragment"
        app:popUpTo="@id/orderServiceListFragment"
        app:popUpToInclusive="true" />
    <fragment
        android:id="@+id/orderServiceDetailFragment"
        android:name="id.monpres.app.ui.orderservicedetail.OrderServiceDetailFragment"
        android:label=""
        tools:layout="@layout/fragment_order_service_detail">
        <argument
            android:name="orderService"
            app:argType="id.monpres.app.model.OrderService" />
        <argument
            android:name="currentUser"
            app:argType="id.monpres.app.model.MontirPresisiUser"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <action
        android:id="@+id/action_global_orderServiceDetailFragment"
        app:destination="@id/orderServiceDetailFragment"
        app:popUpTo="@id/orderServiceDetailFragment"
        app:popUpToInclusive="true" />
    <fragment
        android:id="@+id/serviceProcessFragment"
        android:name="id.monpres.app.ui.serviceprocess.ServiceProcessFragment"
        android:label="@string/service_process"
        tools:layout="@layout/fragment_service_process" >
        <argument
            android:name="orderServiceId"
            app:argType="string" />
        <action
            android:id="@+id/action_serviceProcessFragment_to_orderServiceDetailFragment"
            app:destination="@id/orderServiceDetailFragment"
            app:popUpTo="@id/serviceProcessFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_serviceProcessFragment_to_orderItemEditorFragment"
            app:destination="@id/orderItemEditorFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_serviceProcessFragment"
        app:destination="@id/serviceProcessFragment"
        app:popUpTo="@id/homeFragment" />
    <fragment
        android:id="@+id/partnerHomeFragment"
        android:name="id.monpres.app.ui.partnerhome.PartnerHomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_partner_home" >
        <action
            android:id="@+id/action_partnerHomeFragment_to_serviceProcessFragment"
            app:destination="@id/serviceProcessFragment" />
        <action
            android:id="@+id/action_partnerHomeFragment_to_orderServiceDetailFragment"
            app:destination="@id/orderServiceDetailFragment" />
        <action
            android:id="@+id/action_partnerHomeFragment_to_orderServiceListFragment"
            app:destination="@id/orderServiceListFragment" />
    </fragment>
    <fragment
        android:id="@+id/partnerSelectionFragment"
        android:name="id.monpres.app.ui.partnerselection.PartnerSelectionFragment"
        android:label="@string/nearest_partners"
        tools:layout="@layout/fragment_partner_selection" />
    <action android:id="@+id/action_global_partnerSelectionFragment" app:destination="@id/partnerSelectionFragment" />
    <fragment
        android:id="@+id/orderItemEditorFragment"
        android:name="id.monpres.app.ui.orderitemeditor.OrderItemEditorFragment"
        android:label="Item Editor"
        tools:layout="@layout/fragment_order_item_editor" >
        <argument
            android:name="orderItems"
            app:argType="id.monpres.app.model.OrderItem[]"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="orderService"
            app:argType="id.monpres.app.model.OrderService" />
    </fragment>
    <fragment
        android:id="@+id/adminHomeFragment"
        android:name="id.monpres.app.ui.adminhome.AdminHomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_admin_home" >
        <action
            android:id="@+id/action_adminHomeFragment_to_adminNewUsersFragment"
            app:destination="@id/adminNewUsersFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminNewUsersFragment"
        android:name="id.monpres.app.ui.adminnewusers.AdminNewUsersFragment"
        android:label="@string/new_users"
        tools:layout="@layout/fragment_admin_new_users" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="id.monpres.app.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <action
        android:id="@+id/action_global_settingsFragment"
        app:destination="@id/settingsFragment" />
</navigation>